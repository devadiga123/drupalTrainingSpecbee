{
  "services": {
    "sendOtp": {
      "url": "https://api.gupshup.io/sm/api/ent/sms/2fa/sendOTP",
      "method": "POST",
      "params": [
        "key",
        "phone"
      ]
    },
    "verifyOtp": {
      "url": "https://api.gupshup.io/sm/api/ent/sms/2fa/verifyOTP",
      "method": "POST",
      "params": [
        "key",
        "phone",
        "code"
      ]
    },
    "register": {
      "url": "https://api.gupshup.io/sm/api/ent/sms/account",
      "method": "PUT",
      "params": [
        "accountid",
        "token"
      ]
    },
    "updateMsg": {
      "url": "https://api.gupshup.io/sm/api/ent/sms/2fa/updateMessage",
      "method": "PUT",
      "params": [
        "key",
        "message"
      ]
    },
    "createApiKey": {
      "url": "https://api.gupshup.io/sm/api/ent/sms/2fa/create",
      "method": "POST",
      "params": [
        "message",
        "accountType"
      ]
    },
    "statusCode": {
      "908": {
        "status": "error",
        "response": "Phone number has already been verified"
      },
      "909": {
        "status": "error",
        "response": "International Service is Disabled"
      },
      "904": {
        "status": "error",
        "response": "Inactive Key"
      },
      "910": {
        "status": "error",
        "response": "Voice Service is Disabled"
      },
      "905": {
        "status": "error",
        "response": "Maximum number of attempts exceeded. Kindly try after some time."
      },
      "903": {
        "status": "error",
        "response": "Code is incorrect. Please try again with a valid code."
      },
      "907": {
        "status": "error",
        "response": "Code is expired, please enter the new code."
      },
      "906": {
        "status": "error",
        "response": "Please try after some time."
      },
      "200": {
        "status": "success",
        "response": "Code matched successfully, verification completed."
      },
      "100": {
        "status": "success",
        "response": "We have sent you a new code."
      },
      "103": {
        "status": "success",
        "response": "Previous code is expired and we have sent you a new code."
      },
      "101": {
        "status": "success",
        "response": "We have sent you a new code again."
      }
    }
  }
}